{
  "manifest_version": 2,
  "name": "Sample Extension",
  "version": "0.0.1",
  "description": "Powered by Grommet",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/16.png",
      "24": "icons/24.png",
      "32": "icons/32.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": []
}
